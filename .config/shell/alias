alias \
\
cd="z" \
c="clear" \
v="nvim" \
cat="bat" \
l="ls -h --color=auto --group-directories-first" \
ll="ls -lh --color=auto --group-directories-first" \
ls="ls -h --color=auto --group-directories-first" \
la="ls -lah --color=auto --group-directories-first" \
grep="rg --color=auto" \
shell="exec $SHELL -l" \
fk="sudo !!" \
mv="mv -i" \
rm="rm -Iv" \
df="df -h" \
du="du -h -d 1" \
k="killall" \
p="ps aux | grep $1" \
yay="paru" \
lz="lazygit" \
i="$HOME/scripts/install.sh"
